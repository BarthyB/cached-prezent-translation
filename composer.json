{
  "name": "barthy/cached-prezent-translation",
  "version": "1.0.1",
  "description": "Provides a trait to be used with prezent/translatable-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Barthy",
      "email": "post@barthy.koeln"
    }
  ],
  "keywords": [
    "translatable",
    "cached"
  ],
  "homepage": "https://github.com/BarthyB/cached-prezent-translation",
  "type": "library",
  "require": {
    "php": "^7.1",
    "prezent/doctrine-translatable-bundle": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpmd/phpmd": "^2.9",
    "squizlabs/php_codesniffer": "^3.5",
    "brainmaestro/composer-git-hooks": "^2.8"
  },
  "extra": {
    "hooks": {
      "pre-commit": ["bin/pre-commit"]
    }
  },
  "autoload": {
    "psr-4": { "Barthy\\CachedPrezentTranslation\\": "lib/Barthy/CachedPrezentTranslation/" }
  }
}
